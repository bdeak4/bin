#!/bin/sh

curl -s https://slobodnadalmacija.hr/split |
grep article-link |
head -1 |
sed -e 's/<a .*href=['"'"'"]//' -e 's/["'"'"'].*$//' |
sed 's/^/https:\/\/slobodnadalmacija.hr/'

echo

curl -s https://slobodnadalmacija.hr/vijesti |
grep article-link |
head -1 |
sed -e 's/<a .*href=['"'"'"]//' -e 's/["'"'"'].*$//' |
sed 's/^/https:\/\/slobodnadalmacija.hr/'

echo

curl -s 'https://www.telegram.hr/' -H 'User-Agent: firefox' |
grep 'role="article"' |
head -1 |
sed -e 's/.*href=['"'"'"]//' -e 's/["'"'"'].*$//' |
sed 's/^/https:\/\/archive.is\/newest\/https:\/\/www.telegram.hr/'

echo

curl -s https://www.ft.com/technology |
grep js-teaser-heading-link |
sed -e 's/<a /\n<a /g' |
sed -e 's/<a .*href=['"'"'"]//' -e 's/["'"'"'].*$//' -e '/^$/ d' |
grep content |
uniq |
head -1 |
sed 's/^/https:\/\/archive.is\/newest\/https:\/\/www.ft.com/'

echo

curl -s https://www.ft.com/companies |
grep js-teaser-heading-link |
sed -e 's/<a /\n<a /g' |
sed -e 's/<a .*href=['"'"'"]//' -e 's/["'"'"'].*$//' -e '/^$/ d' |
grep content |
uniq |
head -1 |
sed 's/^/https:\/\/archive.is\/newest\/https:\/\/www.ft.com/'
